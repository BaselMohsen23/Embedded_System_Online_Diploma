.section .vectors 
/* here to reserve a section and include in it all the below of it content until it founds another command */

.word	0x20001000  	/* stack_top adress */
.word 	_reset			/* 1 Reset */
.word 	vector_handler	/* 2 NMI */
.word 	vector_handler	/* 3 Hard Fault */
.word 	vector_handler	/* 4 MM */
.word 	vector_handler	/* 5 Bus Fault */
.word 	vector_handler	/* 6 Usage Fault */
.word 	vector_handler	/* 7 RESERVED */
.word 	vector_handler	/* 8 RESERVED */
.word 	vector_handler	/* 9 RESERVED */
.word 	vector_handler	/* 10 RESERVED */
.word 	vector_handler	/* 11 SV call */
.word 	vector_handler	/* 12 Debug reserved */
.word 	vector_handler	/* 13 RESERVED */
.word 	vector_handler	/* 14 PendSV */
.word 	vector_handler	/* 15 SysTick */
.word 	vector_handler	/* 16 IRQ0 */
.word 	vector_handler	/* 17 IRQ1 */
.word 	vector_handler	/* 18 IRQ2 */
.word 	vector_handler	/* 19 IRQ3 */
	/* on to IRQ67 */

.section .text /* here is the other command in line 7 */

_reset:
	bl main
	b . /*here if main don't have while(1) i tell him to branch here . means in this section*/

.thumb_func /* here to activate the thumb mode of 32 and 16 bit  */ 
vector_handler:
	b _reset